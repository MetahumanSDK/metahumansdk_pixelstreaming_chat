{
  "name": "ballancer-server",
  "version": "1.0.0",
  "description": "ballancer web server without interface example",
  "scripts": {
    "start": "npm install && npm run pack-server && node ./ballancer-server-bundled.js",
    "pack-server": "webpack --config ./webpack-server.config.js --progress --mode=production",
    "watch-server": "webpack --config ./webpack-server.config.js --watch --progress --mode=development"
  },
  "dependencies": {
    "@pixstream/ballancer": "^1.0.2",
    "@pixstream/server": "^1.0.2",
    "@pixstream/universal": "^1.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "webpack": "^5.73.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^14.15.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack-cli": "^4.10.0"
  }
}
